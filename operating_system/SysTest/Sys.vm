function Sys.init 0
	call Memory.init 0
	pop temp 0
	call Keyboard.init 0
	pop temp 0
	call Math.init 0
	pop temp 0
	call Screen.init 0
	pop temp 0
	call Output.init 0
	pop temp 0
	call Main.main 0
	pop temp 0
	push constant 0
	return
function Sys.halt 0
label LOOP0
	push constant 0  // true
	not  // true
	not
	if-goto ENDLOOP0
	goto LOOP0
label ENDLOOP0
	push constant 0
	return
function Sys.wait 1
label LOOP1
	push argument 0  // duration
	push constant 0
	gt
	not
	if-goto ENDLOOP1
	push constant 500
	pop local 0  // delay
label LOOP2
	push local 0  // delay
	push constant 0
	gt
	not
	if-goto ENDLOOP2
	push local 0  // delay
	push constant 1
	sub
	pop local 0  // delay
	goto LOOP2
label ENDLOOP2
	push argument 0  // duration
	push constant 1
	sub
	pop argument 0  // duration
	goto LOOP1
label ENDLOOP1
	push constant 0
	return
function Sys.error 0
	push constant 4
	call String.new 1
	push constant 69
	call String.appendChar 2
	push constant 82
	call String.appendChar 2
	push constant 82
	call String.appendChar 2
	push constant 60
	call String.appendChar 2
	call Output.printString 1
	pop temp 0
	push argument 0  // errorCode
	call Output.printInt 1
	pop temp 0
	push constant 62
	call Output.printChar 1
	pop temp 0
	push constant 0
	return
